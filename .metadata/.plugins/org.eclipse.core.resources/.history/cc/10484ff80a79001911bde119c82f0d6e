package Collection;

import java.util.HashMap;

public class HashMapIteration {

	public static void main(String[] args) {
		
		HashMap<Integer, String> map = new HashMap<Integer, String>();
		
		map.put(01, "Sandesh");
		map.put(02, "Poonam");
		map.put(03, "Chintu");
		
		System.out.println(map.size());

	}

}
